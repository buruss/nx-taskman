          }
        }
      }
    },
    "www-api": {
      "root": "apps/www-api",
      "sourceRoot": "apps/www-api/src",
      "projectType": "application",
      "prefix": "www-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/www-api",
            "main": "apps/www-api/src/main.ts",
            "tsConfig": "apps/www-api/tsconfig.app.json",
            "assets": ["apps/www-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/www-api/src/environments/environment.ts",
                  "with": "apps/www-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "www-api:build"
          },
          "configurations": {
            "production": {}
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/www-api/.eslintrc",
            "tsConfig": [
              "apps/www-api/tsconfig.app.json",
              "apps/www-api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/www-api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/www-api/jest.config.js",
            "tsConfig": "apps/www-api/tsconfig.spec.json"
          }
        }
      }
    },
    "www": {
      "root": "apps/www",
      "sourceRoot": "apps/www/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/www",
            "index": "apps/www/src/index.html",
            "main": "apps/www/src/main.tsx",
            "polyfills": "apps/www/src/polyfills.ts",
            "tsConfig": "apps/www/tsconfig.app.json",
            "assets": ["apps/www/src/favicon.ico", "apps/www/src/assets"],
            "styles": ["apps/www/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/babel"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/www/src/environments/environment.ts",
                  "with": "apps/www/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "www:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "www:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/www/.eslintrc",
            "tsConfig": [
              "apps/www/tsconfig.app.json",
              "apps/www/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/www/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/www/jest.config.js",
            "tsConfig": "apps/www/tsconfig.spec.json"
          }
        }
      }
    },
    "www-e2e": {
      "root": "apps/www-e2e",
      "sourceRoot": "apps/www-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/www-e2e/cypress.json",
            "tsConfig": "apps/www-e2e/tsconfig.e2e.json",
            "devServerTarget": "www:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "www:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/www-e2e/.eslintrc",
            "tsConfig": ["apps/www-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/www-e2e/**"]
